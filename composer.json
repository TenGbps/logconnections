{
	"name": "david63/logconnections",
	"type": "phpbb-extension",
	"description": "This extension will log user connections, failed logins and new users.",
	"version": "2.1.0-RC6",
	"time": "2020-05-18",
		"keywords": [
		"phpbb",
		"extension",
		"log",
		"connection",
		"user"
	],
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "Log user connections",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.8"
      },
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "log_connections_version_file.json",
			"ssl": false
    	}
	}
}
